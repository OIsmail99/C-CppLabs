#include <iostream>

using namespace std;
class Node{
    public:
    int data;
    Node * next;
    Node * prev;

    Node(int data){
    this->data = data;
    this->next = nullptr;
    this->prev = nullptr;
    }

};

class LinkedList{
    Node * head;
    Node * tail;
    int counter;
public:
    LinkedList(){
        head = nullptr;
        tail = nullptr;
        counter = 0;
    }
    void append(int data){
    Node *newNode = new Node(data);
    if(head == nullptr){
        head = newNode;
        tail = newNode;
    }
    else{
        newNode->prev = tail;
        tail->next = newNode;
        tail = newNode;
    }
    this->counter++;
    }
    void display(){
    Node * temp = head;
    while(temp != nullptr){
        cout<<temp->data<<endl;
        temp = temp->next;
    }
    }
    void insertBefore(int before, int data){
    }

    void insertAfter(int after, int data){
    }

};


int main()
{
    LinkedList l;
    l.append(0);
    l.append(10);
    l.append(2);
    l.display();
    return 0;
}

#include <iostream>

using namespace std;

bubbleSort(int arr[], int size){

for(int i = size -1; i > 0; i--){
    bool swapped = false;
    for(int j = 0; j <= i; j++){
        if(arr[j] > arr[j+1]){
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
                swapped = true;
        }
    }
    if(swapped == false) break;
}
}

int main()
{
    int size = 5;
    int arr[] = {2,1,5,-3,4};
    bubbleSort(arr,size);
    for(int i = 0; i < size; i++){
        cout<<arr[i]<<endl;
    }
    return 0;
}
